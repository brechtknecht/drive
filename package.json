{
  "name": "@brechtknecht/drive",
  "version": "1.0.1",
  "description": "A terminal utility to bookmark and quickly navigate to frequently used directories",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "drive": "./dist/index.js"
  },
  "files": [
    "dist",
    "drive.sh",
