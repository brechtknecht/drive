  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brechtknecht/drive.git"
  },
  "homepage": "https://github.com/brechtknecht/drive#readme",
  "bugs": {
    "url": "https://github.com/brechtknecht/drive/issues"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "enquirer": "^2.4.1"
  }
}
